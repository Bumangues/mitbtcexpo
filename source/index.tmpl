  <!-- GOOGLE WEB FONTS -->
  <link href='http://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
</head>

<body>
  <!-- NAVIGATION -->


  Copy
  <nav id="navigation" class="navbar navbar-inverse fixed-top">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="./">MIT BITCOIN EXPO</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div id="navbar" class="navbar-collapse collapse" aria-expanded="false">
      <ul class="nav navbar-nav">
        <li><a href="#registration">Registration</a>
          <li><a href="#speakers">Speakers</a></li>
          <!--<li><a href="#page-schedule">Schedule</a>
        </li>-->
        <li><a href="#page-contact">Contact</a></li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </nav>

  <!-- END NAVIGATION -->

  <!-- PAGE | WELCOME -->
  <div class="pages white page-welcome" id="page-welcome">
    <div class="center">
      <!--logo here, when we have a logo -->
      <div class="line-divider" id="banner">
        <h1>MIT Bitcoin Expo</h1>
      </div>
      <h2> March 05 - 06, 2016</h2>
      <h5>Location: <a href="http://sambergconferencecenter.mit.edu">Building E52: The Samberg Center</a></h5>

      <div class="social-icons sicon-white bordered">
        <a href="https://www.facebook.com/MITBitcoinClub" class="sicon-facebook"><i>Facebook</i></a>
        <a href="https://twitter.com/mitbitcoinclub" class="sicon-twitter"><i>Twitter</i></a>
        <a href="https://www.youtube.com/user/MITBitcoinClub" class="sicon-youtube"><i>Youtube</i></a>
      </div>
    </div>
  </div>
  <!-- END PAGE | Welcome -->

  <!-- PAGE | SPEAKERS -->
  <div class="pages black page-speakers" id="page-speakers">
    <div class="container">
      <header id="registration">
        <h4 class="line-divider">Registration</h4>
        <a href="https://www.eventbrite.com/e/mit-bitcoin-expo-2016-tickets-20991924411">
          <button id="register">&rarr; here &larr;</button>
        </a>
      </header>
      <header id="speakers">
        <h4 class="line-divider">2016 Expo Speakers</h4>
        <p>
          This page will be updated as more speakers confirm
        </p>
      </header>
      <article>
        <!-- List of speakers and short bios-->
        <div id="speakerlist">
          {% for index, (image, name, byline, color, bio) in enumerate(speakers) %}
          {% if index %3 == 0 %}
          <div class="thumbnails about-items row">
            {% end %}
            <div class="col-md-4 center">
              <img class="img-circle col-md-12" style="border-color:{{color}};background:{{color}}" src="{{image}}" alt="MIT BITCOIN EXPO">
              <h5>
                {{name}}<br/>
                <small>{{byline}}</small>
              </h5>
              <p class="smallFontBy08"> {{bio}}</p>
            </div>
            {% if index %3 == 2 %}
          </div>
          {% end %}
          {% end %}
          {% if len(speakers) %3 != 2 %}
        </div>
        {% end %}
      </div>
    </article>
  </div>
</div>
<!-- END PAGE | SPEAKERS -->

